{
  "name": "ng-di-graph",
  "version": "0.1.0",
  "description": "Angular DI dependency graph CLI tool",
  "main": "dist/cli/index.cjs",
  "bin": {
    "ng-di-graph": "dist/cli/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "dev:node": "tsx src/cli/index.ts",
    "build:bundle": "tsdown --config tsdown.config.ts",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build": "npm run build:bundle && npm run build:types",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format src --write",
    "format:check": "biome format src",
    "check": "npm run lint && npm run typecheck",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "angular",
    "dependency-injection",
    "typescript",
    "cli",
    "graph"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.10",
    "rimraf": "^6.0.0",
    "tsdown": "^0.16.6",
    "tsx": "^4.8.1",
    "typescript": "^5.0.0",
    "vitest": "^4.0.10"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "ts-morph": "^27.0.2"
  },
  "engines": {
    "node": ">=20.19.0 <21.0.0 || >=22.12.0"
  }
}
